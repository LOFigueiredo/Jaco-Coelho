						}
						operations {
							if(<strPreliminares.alegaJG>) {
								print #parag# & <contTopic> & "." & <cont> & " seja acolhida a preliminar de impugnação da assistência judiciária gratuita para " & |vectAutor|.grammar("a parte autora") & ", uma vez que não foi comprovada sua hipossuficiência financeira;",
								<cont> = <cont> + 1,
								print #bl#
							},

							print #parag# & <contTopic> & "." & <cont> & " sejam julgados improcedentes os pedidos iniciais de suspensão da execução extrajudicial, retirada do nome " & |vectAutor|.grammar("da parte autora") & " do cadastro de inadimplentes, revisão contratual" & printIf(<strMerito.repeticaoDeindebito>, ", repetição de indébito", null) & ", compensação de valores" & printIf(<strMerito.tutelaProvisoria>, ", tutela antecipada", null) & printIf(<strMerito.inversaoOnus>, " e inversão do ônus da prova", null)	& ", conforme razões expostas;",
							<cont> = <cont> + 1,
							print #bl#,

							print #parag# & <contTopic> & "." & <cont> & " seja deferida a produção de todos os meios de provas admitidos em direito;",
							<cont> = <cont> + 1,
							print #bl#,

							print #parag# & <contTopic> & "." & <cont> & " seja determinado o desentranhamento dos documentos juntados " & |vectAutor|.grammar("pela parte autora") & " em cópia simples;",
							<cont> = <cont> + 1,
							print #bl#,

							print #parag# & <contTopic> & "." & <cont> & " as intimações sejam efetuadas via Diário Oficial conforme determina o artigo 205, § 3º do CPC e a Resolução n. 234/2016 do CNJ, exclusivamente em nome do advogado Jacó Carlos Silva Coelho, inscrito na OAB/GO n. 13.721, endereço de e-mail intimacao@jacocoelho.com.br, sob pena de nulidade nos termos do disposto nos § 2º e 5º do artigo 272 e artigo 276 do mesmo diploma legal.",
							<cont> = <cont> + 1,
							print #bl#,

							print #parag# & <contTopic> & "." & <cont> & " finalmente, se proceda a juntada dos documentos abaixo enumerados, os quais são declarados autênticos e conferem com os originais, nos termos do Art. 425, IV do NCPC:" & #bl#,
							use BRC_Docs_Pedidos_Imobliario,
							<cont> = <cont> + 1
						}
					},
					branch[BRC_Pedidos_Veiculo] {
						declarations {
							-<cont> : Integer {default = 1},
							branch[BRC_Docs_Pedidos_Veiculo] {
								declarations {
									-<contDoc> : Integer {default = 1}
								}
								operations {
									if("Procuração" IN <strDocumentos.documentos>) {
										print #parag# & fontSize(<contTopic> & "." & <cont> & "." & <contDoc> & " Procuração;", "10pt"),
										<contDoc> = <contDoc> + 1
									},
									if("Substabelecimento" IN <strDocumentos.documentos>) {
										print #parag# & fontSize(<contTopic> & "." & <cont> & "." & <contDoc> & " Substabelecimento;", "10pt"),
										<contDoc> = <contDoc> + 1
									},
									if("Contrato" IN <strDocumentos.documentos>) {
										print #parag# & fontSize(<contTopic> & "." & <cont> & "." & <contDoc> & " Contrato;", "10pt"),
										<contDoc> = <contDoc> + 1
									},
									if("Planilha de evolução do contrato" IN <strDocumentos.documentos>) {
										print #parag# & fontSize(<contTopic> & "." & <cont> & "." & <contDoc> & " Planilha de evolução do contrato;", "10pt"),
										<contDoc> = <contDoc> + 1
									}
								}
							}
						}
						operations {
							if(<strPreliminares.valoresIncontroversos>) {
								if(<strPreliminares.ocorreuDeposito> == false) {
									print #parag# & <contTopic> & "." & <cont> & " seja acolhida a preliminar de inépcia da inicial e extinta a presente ação;",
									<cont> = <cont> + 1,
									print #bl#
								}
							},

							if(<strMerito.tutelaProvisoria>) {
								print #parag# & <contTopic> & "." & <cont> & " seja mantido o indeferimento dos pedidos de tutela provisória de urgência, ante a necessidade de dilação probatória, conforme já reconhecido por este Douto Juízo;",
								<cont> = <cont> + 1,
								print #bl#
							},

							print #parag# & <contTopic> & "." & <cont> & " sejam julgados improcedentes os pedidos iniciais de alteração das cláusulas e de consignação de pagamento, bem como a manutenção da decisão que julgou improcedente a tutela jurisdicional, conforme razões expostas;",
							<cont> = <cont> + 1,
							print #bl#,

							print #parag# & <contTopic> & "." & <cont> & " seja deferida a produção de todos os meios de provas admitidos em direito;",
							<cont> = <cont> + 1,
							print #bl#,

							print #parag# & <contTopic> & "." & <cont> & " seja determinado o desentranhamento dos documentos juntados " & |vectAutor|.grammar("pela parte autora") & " em cópia simples;",
							<cont> = <cont> + 1,
							print #bl#,

							print #parag# & <contTopic> & "." & <cont> & " as intimações sejam efetuadas via Diário Oficial conforme determina o artigo 205, § 3º do CPC e a Resolução n. 234/2016 do CNJ, exclusivamente em nome do advogado Jacó Carlos Silva Coelho, inscrito na OAB/GO n. 13.721, endereço de e-mail intimacao@jacocoelho.com.br, sob pena de nulidade nos termos do disposto nos § 2º e 5º do artigo 272 e artigo 276 do mesmo diploma legal.",
							<cont> = <cont> + 1,
							print #bl#,

							print #parag# & <contTopic> & "." & <cont> & " finalmente, se proceda a juntada dos documentos abaixo enumerados, os quais são declarados autênticos e conferem com os originais, nos termos do Art. 425, IV do NCPC:" & #bl#,
							use BRC_Docs_Pedidos_Veiculo,
							<cont> = <cont> + 1,
							print #bl#
						}
					}	
				}
				operations {
					print align(bold(<contTopic> & " Pedido"), "center") & #bl#,
					print #parag# & "Ante o exposto requer que" & #bl#,
					if(<strModelo.modelo> == "Revisional imobiliária") {
						use BRC_Pedidos_Imobliario
					} else {
						use BRC_Pedidos_Veiculo
					},
					<contTopic> = <contTopic> + 1,
					print #bl#
				}	
			},
			branch[BRC_Encerramento] {
				declarations {
					-|advogadoQueAssina| : Vector[String] {
						name = "Advogados que assinam a peça"
					}
				}
				operations {
					foreach(<advogado> IN |strEncerramento.listaDeAdvogados|) {
						|advogadoQueAssina|.add(<advogado>.getByKey("Nome") & "\b" & <advogado>.getByKey("OAB"))
					},
					print #parag# & align("Termos em que, pede deferimento.", "center") & #bl#,
					print #parag# & align(firstUpper(<strEncerramento.localAssinatura>) & "/" & <strEncerramento.estado>.getByKey("Sigla") & ", " & expandedDate(<strEncerramento.dataPeticao>) & ".", "center") & #bl#,

					print "<table class='advsAssinam'>",
					foreach(<i> IN [0:|advogadoQueAssina|.size() - 1]) {
						if (<i>%2 == 1) { 
							print "<tr>",
								print "<td style='text-align:left'>" & <advogadoQueAssina{<i> - 1}> & "\b\b" & "</td>",
								print "<td style='text-align:right'>" & <advogadoQueAssina{<i>}>  & "\b\b" & "</td>",
							print "</tr>"
						}
						elseif (<i> == |advogadoQueAssina|.size() - 1) {
							print "<tr>",
								print "<td style='text-align:center' colspan='2'>" & <advogadoQueAssina{<i>}> & "</td>",
							print "</tr>"
						} 
					},
					print "</table>"
				}
			}
 		}
		operations {
			use BRC_Estilo,
			use BRC_Enderecamento,
			use BRC_Qualificacao,
			if(<strModelo.modelo> == "Revisional imobiliária") {
				if(<strPreliminares.alegaJG>) {
					use BRC_Preliminares
				}
			} else {
				if(<strPreliminares.valoresIncontroversos>) {
					if(<strPreliminares.ocorreuDeposito> == false) {
						use BRC_Preliminares
					}
				}
			},
			use BRC_BreveHistorico,
			use BRC_Merito,
			use BRC_Pedidos,
			use BRC_Encerramento
		}
 	}
 	foot {
 		declarations {
 			branch[TOC_InformacoesGerais] {
				name = "Informações gerais"
				index = true
				operations {
					<strModelo>.ask(),
					<strDadosDoProcesso>.ask()
				}	
			},
			branch[TOC_Partes] {
				name = "Partes"
				index = true
				operations {
					|vectAutor|.ask()
					//|vectReu|.ask()
				}	
			},
			branch[TOC_Preliminares_Fatos] {
				name = "Preliminares e fatos"
				index = true
				operations {
					<strPreliminares>.ask(),
					<strBreveHistorico>.ask()
				}	
			},
			branch[TOC_InformacoesMerito] {
				name = "Informações de mérito"
				index = true
				operations {
					<strMerito>.ask(),
					<strInfoContratuais>.ask(),
					if(<strModelo.modelo> == "Revisional imobiliária") {
						upload("Upload da planilha de evolução", "imagemPlanilhaDeEvolucao.png", "Faça o upload da planilha de evolução", "+")
					}
				}	
			},
			branch[TOC_Encerramento] {
				name = "Encerramento"
				index = true
				operations {
					<strDocumentos>.ask(),
					<strEncerramento>.ask()
				}	
			}
 		}
 		operations {
 			use TOC_InformacoesGerais,
 			use TOC_Partes,
 			use TOC_Preliminares_Fatos,
 			use TOC_InformacoesMerito,
 			use TOC_Encerramento
 		}
 	}
