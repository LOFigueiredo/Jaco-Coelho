									},
									if("Substabelecimento" IN <strDocumentos.documentos>) {
										print #parag# & fontSize(<contTopic> & "." & <cont> & "." & <contDoc> & " Substabelecimento;", "10pt"),
										<contDoc> = <contDoc> + 1
									},
									if("Contrato" IN <strDocumentos.documentos>) {
										print #parag# & fontSize(<contTopic> & "." & <cont> & "." & <contDoc> & " Contrato;", "10pt"),
										<contDoc> = <contDoc> + 1
									},
									if("Ficha de cadastro da parte autora e protocolo de entrega da planilha" IN <strDocumentos.documentos>) {
										print #parag# & fontSize(<contTopic> & "." & <cont> & "." & <contDoc> & " Ficha de cadastro da parte autora e protocolo de entrega da planilha;", "10pt"),
