template[TEMP_ContestacaoRevisional_Imobiliario_Veiculo] { 
	metainfo { 
		language = "pt_BR"
		name = "Contestação em Ação Revisional (Imobiliário e veículo)"
		style = "STY_ContestacaoAcaoRevisonal.lawsty"
		declarations {
			//Constantes
			-#parag# : String {value = "<p class = 'paragrafo'>"},
			-#bl# : String {value = "</p>\b\b"},
			-<contTopic> : Integer {default = 1},
			//Listas
			list[ListaAdvogados_JacoCoelho] {
				fields = {"Nome", "OAB"}
				options = ({"Jacó Carlos Silva Coelho", "OAB/GO 13.721"},
										{"Allinne Rizzie Coelho Oliveira Garcia","OAB/GO 24.549"}, 
										{"Claudinéia Santos Pereira","OAB/GO 22.376"},
										{"Lucimer Coelho de Freitas","OAB/GO 33.001"},
										{"Fabiane Gomes Pereira","OAB/GO 30.485"},
										{"Daniele de Faria Ribeiro Gonzaga","OAB/GO 36.528"})
				type = "String"
			},
			//Estruturas
			+<strModelo> : Struct {
				name = "Modelo"
				fields {
					+[modelo] : List ("Revisional imobiliária", "Revisional veículo") {
						name = "Modelo"
						request = "Selecione o modelo da defesa"
						atomic = true
					}
				}
			},
			+<strDadosDoProcesso> : Struct {
				name = "Dados do processo"
				fields {
					+[numero] : String where ("\d\d\d\d\d\d\d-\d\d.\d\d\d\d.\d.\d\d.\d\d\d\d") {
						name = "Número do processo"
					},
					+[ehVaraOuJuizado] : List ("Juizado especial federal", "Vara federal") {
						name = "A ação foi proposta em vara federal ou juizado especial federal?"
						atomic = true
					},
					+[vara] : Integer where ("\d\r") {
						name = "Vara"
					},
					+[comarca] : String {
						name = "Comarca"
					},
					+[uf] : List [*Uf] {
						name = "Estado de tramitação"
						atomic = true
					}
				}
			},
			+|vectAutor| : Vector[Struct] {
				name = "Autor"
				id = [nome] 
				fields {
					+[nome] : String {
						name = "Nome"
						request = "Informe o nome do autor"
					},
					+[tipoPessoa] : List ("Pessoa Física", "Pessoa Jurídica") {
						name = "Tipo de pessoa"
						request = "Selecione o tipo de pessoa"
						atomic = true
						default = "Pessoa Jurídica"
					},
					-[genero] : List ("Feminino", "Masculino") {
						name = "Gênero"
						request = "Selecione o gênero do autor"
						help = "É necessário selecionar o gênero para que as flexões sejam feitas corretamente"
						atomic = true 
						default = "Feminino"
					}
				}
				loaders {
					if([tipoPessoa] == "Pessoa Jurídica") {
						[genero].default = "Feminino"
					} else {
						[genero].mandatory = "+"
					}
				}
			},
			/*+|vectReu| : Vector[Struct] {
				name = "Réu"
				fields {
					+[nome] : String {
						name = "Nome"
						request = "Informe o nome do réu"
					},
					+[tipoPessoa] : List ("Pessoa Física", "Pessoa Jurídica") {
						name = "Tipo de pessoa"
						request = "Selecione o tipo de pessoa"
						atomic = true
						default = "Pessoa Jurídica"
					},
					if([tipoPessoa] == "Pessoa Jurídica") {
						+[cnpj] : String where ("CNPJ") {
							name = "CNPJ"
							request = "Informe o CNPJ do réu"
						}
					} else {
						+[cpf] : String where ("CPF") {
							name = "CPF"
							request = "Informe o CPF do réu"
						}
					},
					-[genero] : List ("Feminino", "Masculino") {
						name = "Gênero"
						request = "Selecione o gênero do réu"
						help = "É necessário informar o gênero para que as flexões sejam feitas corretamente"
						atomic = true 
					}
				}
				loaders {
					[nome].default = "Caixa Econômica Federal",
					if([tipoPessoa] == "Pessoa Jurídica") {
						[genero].default = "Feminino",
						[cnpj].default = "00.360.305/0001-04"
					} else {
						[genero].mandatory = "+"
					}
				}
			},*/
			+<strPreliminares> : Struct {
				name = "Preliminares"
				fields {
					if([Metainfo:strModelo.modelo] == "Revisional imobiliária") {
						+[alegaJG] : Boolean {
							name = "Justiça gratuita"
							request = "O autor requereu justiça gratuita?"
						},
						if([alegaJG]) {
							+[juntouDeclaracao] : Boolean {
								name = "Declaração de hipossuficiência"
								request = "O autor juntou declaração de hipossuficiência?"
							},
							+[temAdvParticular] : Boolean {
								name = "Declaração de hipossuficiência"
								request = "O autor está representado por advogado particular?"
							}
						}
					} else {
						+[valoresIncontroversos] : Boolean {
							name = "Depósito de valores incontroversos"
							request = "Houve alegação de depósito de valores incontroversos?"
						},
						if([valoresIncontroversos]) {
							+[ocorreuDeposito] : Boolean {
								name = "O depósito de fato ocorreu?"
							}
						}
					}
				}
			},
			+<strBreveHistorico> : Struct {
				name = "Breve histórico"
				fields {
					+[valorDaCausa] : Currency {
						name = "Valor da causa"
						request = "Informe o valor da causa"
					},
					if([Metainfo:strModelo.modelo] == "Revisional imobiliária") {
						+[dataDaCelebracao] : Date {
							name = "Data da celebração"
							request = "Informe a data da celebração do contrato"
						}
					} 
				}
			},
			+<strMerito> : Struct {
				name = "Mérito"
				fields {
					+[tutelaProvisoria] : Boolean {
						name = "Tutela provisória"
						request = "Houve pedido de tutela provisória em caráter de urgência?"
					},
					+[inversaoOnus] : Boolean {
						name = "Inversão do ônus da prova"
						request = "Deseja inserir tópico que trata da inversão do ônus da prova?"
					},
					if([Metainfo:strModelo.modelo] == "Revisional imobiliária") {
						+[repeticaoDeindebito] : Boolean {
							name = "Repetição de indébito"
							request = "Houve pedido de repetição de indébito?"
						}
					}	
				}	
			},
			+<strInfoContratuais> : Struct {
				name = "Informações contratuais" 
				fields {
					+[numeroDoContrato] : String {
						name = "Número do contrato"
						request = "Informe o número do contrato"
					},
					+[periodoInadimplencia] : List ("Dias", "Meses", "Anos") {
						name = "Período de inadimplência"
						request = "Informe o período o qual o autor ficou inadimplente"
						atomic = true
					},
					if([Metainfo:strModelo.modelo] == "Revisional imobiliária") {
						+[coberturas] : List ("Morte e invalidez permanente", "Danos físicos no imóvel") {
							name = "Coberturas"
							request = "Selecione a cobertura do contrato"
							atomic = false
						},
						+[operacao] : Currency {
							name = "Operação"
							request = "Informe o valor da operação"
						},
						+[avaliacao] : Currency {
							name = "Avaliação"
							request = "Informe o valor da avaliação"
						},
						+[rendaFamiliar] : Currency {
							name = "Renda familiar"
							request = "Informe o valor da renda familiar"
						},
						+|vectResponsabilidade| : Vector[Struct] {
							name = "Responsabilidade"
							fields {
								+[responsavel] : String {
									name = "Responsável"
									request = "Selecione o responsável"
								},
								+[percentual] : Real where ("[0,100]") {
									name = "Percentual"
									request = "Informe o percentual de responsabilidade"
								}
							}
						},
						+[strEnderecoImovel] : Struct {
							name = "Endereço do imóvel"
							id = [logradouro] & printIf([numero].isNotEmpty(), ", nº " & [numero], null) & printIf([complemento].isNotEmpty(), ", " & [complemento], null) & printIf([bairro].isNotEmpty(), ", " & [bairro], null) & ", " & [cidade] & "-" & [estado].getByKey("Preposição") & " " & [estado] /*& ", CEP: " & [codigoPostal]*/
							fields {
								/*+[codigoPostal] : String where("CEP") {
									name = "CEP"
									request = "CEP"
								},*/
								+[estado] : List[*Uf] {
									name = "Unidade da Federação"
									request = "UF"
									default = "São Paulo"
									atomic = true
								},
								+[cidade] : String {
									name = "Cidade"
									request = "Cidade"
									default = "São Paulo"
								},
								+[logradouro] : String {
									name = "Logradouro"
									request = "Logradouro"
									help = "Exemplo: Avenida Magalhães de Castro"
								},
								&[numero] : String {
									name = "Número"
									request = "Número"
									help = "Por exemplo, se o endereço é Avenida Magalhães de Castro, nº 286, informe \"286\""
								},
								&[complemento] : String {
									name = "Complemento"
									request = "Complemento"
									help = "Exemplo: apartamento 123, sala A, Bloco B"
								},
								&[bairro] : String {
									name = "Bairro"
									request = "Bairro"
									help = "Exemplo: Centro, Bairro Novo"
								}
							}
						},
						+[prestacao] : Currency {
							name = "Prestação"
							request = "Informe o valor da prestação"
							help = "O valor da prestação corresponde ao valor da amortização + juros"
						},
						+[premio] : Currency {
							name = "Prêmio"
							request = "Informe o valor do prêmio"
						},
						+[taxaOperacional] : Currency {
							name = "Taxa operacional"
							request = "Informe o valor da taxa operacional"
						},
						-[total] : Currency,
						+[taxaDeJurosAnual] : Real {
							name = "Taxa de juros anual"
							request = "Informe a taxa de juros anual"
						},
						-[taxaDeJurosMensal] : Real,
						+[pagamentoPorDebitoAutomatico] : Boolean {
							name = "O pagamento ocorreu via débito automático?"
						},
						if([pagamentoPorDebitoAutomatico]) {
							+[taxaDeJurosAnualComDesconto] : Real {
								name = "Taxa de juros anual com desconto"
								request = "Informe a taxa de juros anual com desconto"
							}
						},
						+[exclusaoDaTaxa] : Date {
							name = "Exclusão da taxa"
							request = "Informe a data da planilha que consta a exclusão da taxa"
						},
						+[incorporacaoDeSaldo] : Date {
							name = "Incorporação de saldo"
							request = "Informe a data da planilha que consta a incorporação de saldo"
						},
						+[saldoDevedor] : Currency {
							name = "Saldo devedor"
							request = "Informe o valor do saldo devedor"
						}
					} else {
						+[operacao] : String {
							name = "Operação"
							request = "Informe a operação"
							default = "7044 - Financiamento de Veículos PF"
						},
						+[cdi] : String {
							name = "CDI"
							request = "Informe a CDI"
							default = "4389 - CDI ANUALIZADA - BASE 252"
						}
					}
				}
				loaders {
					if([Metainfo:strModelo.modelo] == "Revisional imobiliária") {
						[total] = [prestacao] + [premio] + [taxaOperacional],
						[taxaDeJurosMensal] = [taxaDeJurosAnual]/12
					}
				}
			},
			+<strDocumentos> : Struct {
				name = "Documentos"
				fields {
					+[documentos] : List ("Procuração", "Substabelecimento", "Contrato", "Ficha de cadastro da parte autora e protocolo de entrega da planilha", "Planilha de evolução das parcelas", "Planilha de evolução do débito, resumo do débito e atraso no pagamento", "Planilha de evolução do contrato") {
						name = "Documentos"
						request = "Selecione os documentos que deseja juntar"
						atomic = false
					},
					-|recebeDocs| : Vector[String]
				}
				loaders {
					if([Metainfo:strModelo.modelo] == "Revisional imobiliária") {
						[documentos].options = {"Procuração", "Substabelecimento", "Contrato", "Ficha de cadastro da parte autora e protocolo de entrega da planilha", "Planilha de evolução das parcelas", "Planilha de evolução do débito, resumo do débito e atraso no pagamento"} 
					} else {
						[documentos].options = {"Procuração", "Substabelecimento", "Contrato", "Planilha de evolução do contrato"}
					}
				}
			},
			+<strEncerramento> : Struct {
				name = "Encerramento"
				fields {
					+[dataPeticao] : Date {
						name = "Data de assinatura"
						default = today()
					},
					+[localAssinatura] : String {
						name = "Local de assinatura"
						request = "Cidade de assinatura"
					},
					+[estado] : List[*Uf] {
						name = "Estado"
						request = "Estado de assinatura"
						atomic = true
					},
					&[listaDeAdvogados] : List[ListaAdvogados_JacoCoelho] {
						name = "Advogado subscritor"
						request = "Selecione o advogado que assinará a petição"
						atomic = false
						default = "Jacó Carlos Silva Coelho"
					}
				}
			}
		} 
	} 
	head { 
		title = null
		operations {
			header("logo_jacoCoelho.png", "center", "70%"),
			footer("foot_jacoCoelho.png", "center","155mm"),
			setDocumentName("Contestação em Ação Revisional - " & <strDadosDoProcesso.numero>)
		} 
 	} 
 	body {
 		declarations {
 			branch[BRC_Estilo] {
				declarations {
					branch[CSS_Titulo] {
						operations {
							print "<style>
								a, h1, h2, h3, h4 {
									text-align: justify;
									margin: 0pt;
									padding: 0pt
								}
							</style>"
						}
					},
					branch[CSS_Paragrafo] {
						operations {
							print "<style>
								.paragrafo {
									text-indent: 30mm;
									margin: 0pt;
									padding: 0pt
								}
							</style>"
						}
					},
					branch[CSS_Citacao] {
						operations {
							print "<style>
								.citation {
									margin: 0pt 0pt 0pt 40mm;
									padding: 0pt 0pt 0pt 0pt
								}
							</style>"
						}
					},
					branch[CSS_Lista] {
						operations {
							print "<style>
								ul, ol, li {
									margin: 0pt 0pt 0pt 60mm;
									padding: 0pt 0pt 0pt 0pt
								}
								.print-content li {
									margin: 0pt 0pt 0pt 0pt;
									padding: 0pt 0pt 0pt 0pt
								}
							</style>"
						}
					},
					branch[CSS_Tabela] {
						operations {
							print "<style>
								table.acordao,
								table.acordao tr {
									border: none;
									padding: 0px;
									width: 100%
								}
								table.acordao td {
									border: 1px solid black;
									text-align: justify;
									padding: 0pt 1.9mm 0pt 1.9mm;
									width: 50%
								}

								table.advsAssinam,
								table.advsAssinam tr {
									border: none;
									padding: 0px;
									width: 100%
								}
								table.advsAssinam td {
									border: none;
									vertical-align: top;
									font-size: 12pt;
									padding: 0pt 1.9mm 0pt 1.9mm;
									width: 50%
								}
							</style>"
						}
					}
				}
				operations {
					use CSS_Titulo,
					use CSS_Paragrafo,
					use CSS_Citacao,
					use CSS_Lista,
					use CSS_Tabela
				}
			},
 			branch[BRC_Enderecamento] {
				operations {
					print align(bold("Ao Juízo " & printIf(<strDadosDoProcesso.ehVaraOuJuizado> == "Juizado especial federal", "do " & <strDadosDoProcesso.vara> & "º Juizado Especial Federal ", "da " & <strDadosDoProcesso.vara> & "ª Vara Federal ") & "da Comarca de " & firstUpper(<strDadosDoProcesso.comarca>) & "/" & <strDadosDoProcesso.uf>.getByKey("Sigla") & "."), "center") & #bl#,
					print #bl#,
					print #bl#,
					print "Protocolo n.:\t\t" & <strDadosDoProcesso.numero> & "\b",
					print "Parte Autora:\t\t" & |vectAutor|.vect2str("%f, %s, %p e %l") & "\b",
					print "Parte Ré:\t\t\tCaixa Econômica Federal" & #bl#,
					print #bl#
				}
			},
			branch[BRC_Qualificacao] {
				operations {
					print #parag# & bold("Caixa Econômica Federal, ") & "Instituição financeira sob a forma de empresa pública, dotada de personalidade jurídica de direito privado, criada pelo Decreto- Lei n.º 759, de 12 de Agosto de 1969, regendo-se atualmente através de seu Estatuto aprovado pelo Decreto nº 5.056/2004, de 29 de abril de 2004, arquivado na Junta Comercial do Distrito Federal, inscrita no CNPJMF sob o n.º 00. 360.305/0001-04, com sede no SBS, Quadra 04, Lote 34, Brasília-DF, por sua Gerência Jurídica Regional - JURIR/GO, situada na Rua 11, n.º 250 – 11º andar – Centro - Goiânia/GO, CEP: 74.015-170, por seus advogados que esta subscrevem, com endereço profissional constante do rodapé da presente, onde o advogado indicado ao final recebe as intimações, vem, à presença de Vossa Excelência, para apresentar " & printIf(<strModelo.modelo> == "Revisional imobiliária", bold("Contestação à AÇÃO REVISIONAL DE CONTRATO" & printIf(<strMerito.tutelaProvisoria>, " COM PEDIDO DE ANTECIPAÇÃO DE TUTELA", null)), bold("Contestação à AÇÃO REVISIONAL C/C COM AÇÃO CONSIGNATÓRIA" & printIf(<strMerito.tutelaProvisoria>, " COM PEDIDO DE TUTELA DE URGÊNCIA CAUTELAR ANTECEDENTE", null))) & ", movida por " & |vectAutor|.vect2str("<b>%f</b>, <b>%s</b>, <b>%p</b> e <b>%l</b>") & ", já " & |vectAutor|.grammar("qualificado") & " na inicial, fazendo-o pelos fatos e fundamentos que se seguem." & #bl#
				}
			},
			branch[BRC_Preliminares] {
				declarations {
					-<cont> : Integer {default = 1},
					-<refAREsp372220> : String {
						default = fontSize(fontFamily("(AgRg no AREsp n. 372.220/RJ, Relator Ministro Ricardo Villas Bôas Cueva, Terceira Turma, julgado em 3/4/2014, DJe 15/4/2014).", "Arial"), "8pt")
					},
					branch[BRC_ImpugnacaoJusticaGratuita] {
						operations {
							print align(bold(<contTopic> & "." & <cont> & " Ausência de Comprovação de Insuficiência de Recursos – Impugnação ao Pedido de Assistência Judiciária"), "center") & #bl#,
							print #parag# & |vectAutor|.grammar("A parte autora") & printIf(|vectAutor|.size() > 1, " insurgem", " insurge") & " com a presente demanda alegando que não " & printIf(|vectAutor|.size() > 1, "possuem", "possui") & " condições financeiras para realizar o pagamento das custas processuais. Contudo, apenas " & printIf(|vectAutor|.size() > 1, "alegam", "alega") & " não ter capacidade de suportar as custas e despesas processuais, sem comprovar insuficiência de recursos para tal, nos termos do art. 98, CPC." & #bl#,
							print #parag# & "É consabido que a declaração de pobreza gera presunção relativa acerca da necessidade da assistência gratuita, ou seja, " & it("juris tantum") & footNote(<refAREsp372220>) & ", podendo o Julgador verificar outros elementos constantes do processo para decidir acerca do deferimento ou não do benefício, uma vez que pode decorrer dos autos a demonstração de que a parte tem condições de arcar com as despesas processuais, o que impediria a concessão deste pedido." & #bl#,
							if(<strPreliminares.juntouDeclaracao> == false) {
								print #parag# & "No caso em apreço, " & |vectAutor|.grammar("a parte autora") & printIf(|vectAutor|.size() > 1, " sequer juntaram", " sequer juntou") & " em sua inicial a declaração de pobreza assinada por "  & |vectAutor|.grammar("ela") & ", ou seja, não há nenhum motivo que embase a Assistência Judiciária Gratuita deferida por este Juízo, sendo que não há comprovação alguma nos autos de que " & printIf(|vectAutor|.size() > 1, "as partes não podem", "a parte não pode") & " suportar o pagamento das custas processuais." & #bl#
							},
							print #parag# & "A comprovação desta condição de hipossuficiência se mostra imprescindível, seja para atender a dispositivo legal, seja para evitar que o Estado seja onerado indevidamente, seja ainda para coibir o ajuizamento sem medida de demandas sob manto do “grátis”." & #bl#,
							if(<strPreliminares.temAdvParticular>) {
								print #parag# & "Ademais, " & |vectAutor|.grammar("a parte autora") & " não " & printIf(|vectAutor|.size() > 1, "provaram", "provou") & " a realização de despesas que justificassem o deferimento do benefício pleiteado e, estando " & |vectAutor|.grammar("representada") & " por advogado particular, é necessária a demonstração da incapacidade financeira. Outrossim, em atenção ao disposto no art. 13, CPC, caso o Autor não possa suportar todas as despesas, deverá comprovar que não pode ao menos ratear os valores." & #bl#
							},
							print #parag# & "Diante do exposto, impugna a assistência judiciária gratuita concedida " & |vectAutor|.grammar("à parte autora") & ", requerendo " & printIf(|vectAutor|.size() > 1, "sejam intimadas", "seja intimada") & " a comprovar nos autos sua condição de hipossuficiência para pagar integral, ou parcialmente, as custas e despesas processuais." & #bl#,
